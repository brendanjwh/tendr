<!DOCTYPE html>
<html>
  <head>
    <title>Tendr</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <div id="logo">
      <div id="sidebar">
        <%= link_to root_path do %>
          <%= image_tag "logo.jpg" %>
        <% end %>
      </div>
    </div>
    <%= yield %>
  </body>
</html>
